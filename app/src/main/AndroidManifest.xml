<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="idn.hnslabs.sqliteexample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="idn.hnslabs.sqliteexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="idn.hnslabs.sqliteexample.BaruActivity"
            android:label="@string/title_activity_baru"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="idn.hnslabs.sqliteexample.MainActivity" />
        </activity>
        <activity
            android:name="idn.hnslabs.sqliteexample.UbahActivity"
            android:label="@string/title_activity_ubah"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="idn.hnslabs.sqliteexample.MainActivity" />
        </activity>
        <activity
            android:name="idn.hnslabs.sqliteexample.LihatActivity"
            android:label="@string/title_activity_lihat"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="idn.hnslabs.sqliteexample.MainActivity" />
        </activity>
    </application>

</manifest>
